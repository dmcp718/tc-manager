{
  "name": "file-explorer-backend",
  "version": "1.0.0",
  "description": "Backend server for file explorer with LucidLink filesystem indexing",
  "main": "server-v2.js",
  "scripts": {
    "start": "node server-v2.js",
    "dev": "nodemon server.js",
    "db:setup": "psql -d sitecache_db -f schema.sql",
    "db:reset": "psql -d sitecache_db -c 'DROP SCHEMA public CASCADE; CREATE SCHEMA public;' && npm run db:setup"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^2.7.0",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "file-explorer",
    "filesystem",
    "websocket",
    "express"
  ],
  "author": "",
  "license": "MIT"
}
